changeOfferZindex:function(nr){for(var i=0;i<o.offersCount;i++){$(e.offer[i]).css('z-index',i===nr?100:i)}},initCanvases:function(){if(!$(e.offer[0]).find('canvas'))return;for(var i=0;i<o.offersCount;i++){canvases[i]=$($(e.offer[i]).find('canvas'))}},addChangeZoomEvent:function(){$(window).resize(function(){if(canvases.length===0||currentZoom==window.devicePixelRatio)return;currentZoom=window.devicePixelRatio;for(var i=0;i<canvases.length;i++){canvases[i].css('image-rendering',currentZoom===1?'pixelated':'auto')}})}}